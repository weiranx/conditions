# Backend runtime
NODE_ENV=production
PORT=3001

# Comma-separated list of allowed browser origins in production.
# Example: https://summitsafe.example.com,https://admin.example.com
CORS_ORIGIN=

# External request behavior
REQUEST_TIMEOUT_MS=9000
AVALANCHE_MAP_LAYER_TTL_MS=600000
SNOTEL_STATION_CACHE_TTL_MS=43200000

# API rate limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=300

# Debug avalanche pipeline logs
DEBUG_AVY=false
